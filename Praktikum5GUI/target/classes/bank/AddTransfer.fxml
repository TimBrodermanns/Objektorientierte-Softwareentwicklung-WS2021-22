<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="bank.controller.AddTransferController"
            prefHeight="400.0" prefWidth="600.0">
    <Label text="Date"/>
    <TextField fx:id="date"/>
    <Label text="Amount"/>
    <TextField fx:id="amount"/>
    <Label text="Description"/>
    <TextField fx:id="des"/>
    <Label text="Recipent"/>
    <ComboBox fx:id="reci"/>
    <Button text="confirm" onMouseClicked="#sendTransfer"/>
</VBox>
